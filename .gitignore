# Ignore directories generated by Composer/bower/npm #
######################################################
node_modules
bower_components
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information#
###############################
/web/sites/*/settings.php
/web/sites/*/settings.local.php
.env

# Ignore Drupal's file directory #
##################################
/web/sites/*/files/

# Ignore SimpleTest multi-site environment. #
#############################################
/web/sites/simpletest

# Ignore files generated by PhpStorm #
######################################
/.idea/

# Compiled source #
###################
*.com
*.class
*.dll
*.exe
*.o
*.so

# Packages #
############
# it's better to unpack these files and commit the raw source
# git has its own built in compression methods
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# Logs and databases #
######################
*.log
*.sql
*.sqlite

# OS generated files #
######################
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
