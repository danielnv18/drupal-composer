# Ignore directories generated by Composer/bower/npm #
######################################################
node_modules
bower_components
/drush/contrib/
/vendor/
/build/

# Ignore sensitive information#
###############################
/src/sites/default/services.local.yml
/src/sites/default/settings.local.php
/.env.local

# Ignore Drupal's file directory #
##################################
/src/sites/*/files

# Ignore files generated by PhpStorm #
######################################
/.idea/

# Compiled source #
###################
*.com
*.class
*.dll
*.exe
*.o
*.so

# Packages #
############
# it's better to unpack these files and commit the raw source
# git has its own built in compression methods
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# Logs and databases #
######################
*.log
*.sql
*.sqlite

# OS generated files #
######################
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
